import java.awt.BorderLayout;

import javax.swing.*; 
public class Frame extends JFrame {
	
	JFrame frame; 
	public Frame() { 
		JTextField tf = new JTextField(); 
		tf.setBounds(50, 100, 200, 30);
		setTitle("Find My Food"); 
		add(tf); 
		setSize(400,500); 
		setLayout(null); 
		setVisible(true); 
	}

}
